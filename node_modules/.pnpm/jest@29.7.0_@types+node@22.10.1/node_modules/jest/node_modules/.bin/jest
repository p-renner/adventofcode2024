#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/phil/projects/adventofcode2024/node_modules/.pnpm/jest@29.7.0_@types+node@22.10.1/node_modules/jest/bin/node_modules:/home/phil/projects/adventofcode2024/node_modules/.pnpm/jest@29.7.0_@types+node@22.10.1/node_modules/jest/node_modules:/home/phil/projects/adventofcode2024/node_modules/.pnpm/jest@29.7.0_@types+node@22.10.1/node_modules:/home/phil/projects/adventofcode2024/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/phil/projects/adventofcode2024/node_modules/.pnpm/jest@29.7.0_@types+node@22.10.1/node_modules/jest/bin/node_modules:/home/phil/projects/adventofcode2024/node_modules/.pnpm/jest@29.7.0_@types+node@22.10.1/node_modules/jest/node_modules:/home/phil/projects/adventofcode2024/node_modules/.pnpm/jest@29.7.0_@types+node@22.10.1/node_modules:/home/phil/projects/adventofcode2024/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
